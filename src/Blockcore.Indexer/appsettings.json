{
   "AllowedHosts": "*",
   "Kestrel": {
      "EndPoints": {
         "Http": {
            "Url": "http://+:9050/"
         }
      }
   },
   "Logging": {
      "LogLevel": {
         "Default": "Information",
         "System": "Warning",
         "Microsoft": "Warning"
      },
      "Console": {
         "IncludeScopes": true
      }
   },
   "Indexer": {
      "ConnectionString": "mongodb://{Symbol}-mongo",
      "DatabaseNameSubfix": false,

      // Communication with RPC endpoints
      // "RpcAccessPort": 5000,
      "RpcSecure": false,
      "RpcDomain": "{Symbol}-client",
      "RpcUser": "rpcuser",
      "RpcPassword": "rpcpassword",

      // Api parameters
      "SyncApiPort": 9050,

      // Notification parameters
      "NotifyUrl": "https://apiurl",
      "NotifyBatchCount": 0,

      // Syncing parameters
      "SyncBlockchain": true,
      "SyncMemoryPool": true,
      "StartBlockIndex": 1,
      "MongoBatchSize": 10000,
      "ParallelRequestsToTransactionRpc": 50,
      "DetailedTrace": 0,
      "MaxItemsInQueue": 200,
      "SyncInterval": 5,
      "AverageInterval": 10,

      // Store the trx hex in mongo storage or read it from RPC
      "StoreRawTransactions": false,

      // Parameters that define the network
      "NetworkConsensusFactoryType": "NBitcoin.ConsensusFactory, NBitcoin",
      "NetworkPubkeyAddressPrefix": 0,
      "NetworkScriptAddressPrefix": 5,
      "NetworkWitnessPrefix": "bc"
   }
}
